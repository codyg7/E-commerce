{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install && cd .. && npm install",
    "build": "cd client && npm run build",
    "test": "cd routes && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codyg7/E-commerce.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codyg7/E-commerce/issues"
  },
  "homepage": "https://github.com/codyg7/E-commerce#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.4",
    "mongoose": "^5.9.22",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4"
  }
}
